    "@ruby/wasm-wasi": "^2.7.2",
    "@typescript/vfs": "^1.6.2",
    "@xterm/addon-fit": "^0.11.0-beta.115",
    "@xterm/xterm": "^5.6.0-beta.115",
    "ace-builds": "^1.43.2",
    "async-mutex": "^0.5.0",
    "better-auth": "^1.3.31",
    "chai": "^6.2.0",
    "chalk": "^5.5.0",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.7",
    "mocha": "^11.7.4",
    "next": "<15.5",
    "pg": "^8.16.3",
    "prismjs": "^1.30.0",
    "pyodide": "^0.29.0",
    "react": "19.1.0",
    "react-ace": "^14.0.1",
    "react-dom": "19.1.0",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^16.1.0",
    "remark-gfm": "^4.0.1",
    "swr": "^2.3.6",
    "typescript": "5.9.3",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/chai": "^5.2.3",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20",
    "@types/pg": "^8.15.5",
    "@types/prismjs": "^1.26.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-syntax-highlighter": "^15.5.13",
    "daisyui": "^5.5.5",
